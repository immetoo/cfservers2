
# Returns the mail addy.
# postfix ignores the return value
#
bind_dn= uid=postfix,ou=services,dc=m4n,dc=nl
bind_pw= superP
server_host=ldaps://ldap1.lan.mbuyu.nl ldaps://ldap3.lan.mbuyu.nl
server_port=636
bind= yes
timeout= 20
search_base= ou=boxes,ou=mail,dc=m4n,dc=nl
query_filter= (&(objectclass=mailAccount)(mail=%s)(mailActive=TRUE))
result_attribute= mail
scope=one
version=3
